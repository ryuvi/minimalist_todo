{
  "expo": {
    "name": "Simple Todo",
    "slug": "minimalist_todo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/prioriza.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/prioriza.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hero.prioriza"
    },
    "web": {
      "favicon": "./assets/prioriza.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
